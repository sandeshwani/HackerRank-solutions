// 1 Arrays - DS reverse the array
import java.util.Scanner;
public class Solution {

    public static void main(String[] args) {
            Scanner sc=new Scanner(System.in);
            int n = sc.nextInt();
            int arr[] = new int[n];
            for(int a=0; a<n; a++)
            {
            	arr[a]= sc.nextInt();
            }
            sc.close();
            for(int a=n-1; a>=0; a--)
            {
            	System.out.print(arr[a] + " ");
            }
    }
}